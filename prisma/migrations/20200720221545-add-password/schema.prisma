generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model Memory {
  id        Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
  location  String?
  title     String
  user      User     @relation(fields: [userId], references: [id])
  userId    Int
}

model User {
  id         Int      @default(autoincrement()) @id
  email      String   @unique
  password   String
  name       String
  memories   Memory[]
}